{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react-boilerplate",
  "main": "index.js",
  "scripts": {
    "clean:build": "rimraf -- build",
    "test": "cross-env NODE_ENV=test mocha --require ignore-styles --require babel-core/register test/index.js \"./src/**/*.test.js\"",
    "dev": "cross-env webpack-dev-server --config webpack.dev.js --progress --inline -d",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js --progress --hide-modules -p"
  },
  "author": "allen79yu",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "bundle-loader": "^0.5.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "chai-jsx": "^1.0.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^3.0.0",
    "jsdom": "^11.4.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
}
